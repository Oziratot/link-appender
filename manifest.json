{
    "name": "Link Appender",
    "description": "Appends the highlighted text to the link and opens it in new tab",
    "version": "0.1",
    "manifest_version": 3,
    "permissions": ["activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "commands": {
      "inject-script": {
        "suggested_key": "Ctrl+Shift+X",
        "description": "Inject a script on the page"
      }
    },
    "icons": {
        "16": "./icons/appender-16.png",
        "32": "./icons/appender-32.png",
        "48": "./icons/appender-64.png",
        "128": "./icons/appender-128.png"
        }
}